Proceso operadores_logicos
	definir booleano1 Como logico;
	definir booleano2 Como logico;
	booleano1 <- Verdadero;
	booleano2 <- Falso;
	Escribir booleano1, " y " , booleano2;
	Escribir booleano1 & booleano2;
	Escribir booleano1, " o " , booleano2;
	Escribir booleano1 | booleano2;
	Escribir "no " , booleano1;
	Escribir ~ booleano1;
FinProceso
